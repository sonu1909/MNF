<Window x:Class="MnfAreaParser.LoadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MnfAreaParser"
        mc:Ignorable="d"
        Title="LoadWindow" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="TB" AcceptsReturn="True"/>
        <Button Content="OK" HorizontalAlignment="Right" VerticalAlignment="Bottom" Width="75" Click="ClickOK"/>

    </Grid>
</Window>
