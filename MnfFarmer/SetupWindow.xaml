<Window x:Class="MnfFarmer.SetupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:mnf="clr-namespace:MnfPic;assembly=MnfPic"
        xmlns:local="clr-namespace:MnfFarmer"
        mc:Ignorable="d"
        Title="SetupWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="200*"/>
        </Grid.RowDefinitions>
        <mnf:MnfLogger x:Name="l" Grid.Row="1"/>
        <ComboBox x:Name="cb" HorizontalAlignment="Left" Width="120" SelectionChanged="cb_SelectionChanged"/>
    </Grid>
</Window>
